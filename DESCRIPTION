Package: webtrackR
Title: Analysing Webtracking Data and Online News Behaviour
Version: 0.0.1
Authors@R: c(
    person("David", "Schoch", email = "david@schochastics.net", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0003-2952-4812")),
